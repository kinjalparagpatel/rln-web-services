<h1>Listing orderable_concepts</h1>

<table>
  <tr>
    <th>Orig orderable concept</th>
    <th>Orderable</th>
    <th>Coding system</th>
    <th>Code</th>
    <th>Active ind</th>
    <th>Begin at</th>
    <th>End at</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @orderable_concepts.each do |orderable_concept| %>
  <tr>
    <td><%= orderable_concept.orig_orderable_concept_id %></td>
    <td><%= orderable_concept.orderable_id %></td>
    <td><%= orderable_concept.coding_system %></td>
    <td><%= orderable_concept.code %></td>
    <td><%= orderable_concept.active_ind %></td>
    <td><%= orderable_concept.begin_at %></td>
    <td><%= orderable_concept.end_at %></td>
    <td><%= link_to 'Show', orderable_concept %></td>
    <td><%= link_to 'Edit', edit_orderable_concept_path(orderable_concept) %></td>
    <td><%= link_to 'Destroy', orderable_concept, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

