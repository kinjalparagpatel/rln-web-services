<h1>Listing org_connections</h1>

<table>
  <tr>
    <th>Orig org connection</th>
    <th>Partner org</th>
    <th>Client org</th>
    <th>Active ind</th>
    <th>Begin at</th>
    <th>End at</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @org_connections.each do |org_connection| %>
  <tr>
    <td><%= org_connection.orig_org_connection_id %></td>
    <td><%= org_connection.partner_org_id %></td>
    <td><%= org_connection.client_org_id %></td>
    <td><%= org_connection.active_ind %></td>
    <td><%= org_connection.begin_at %></td>
    <td><%= org_connection.end_at %></td>
    <td><%= link_to 'Show', org_connection %></td>
    <td><%= link_to 'Edit', edit_org_connection_path(org_connection) %></td>
    <td><%= link_to 'Destroy', org_connection, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Org connection', new_org_connection_path %>
