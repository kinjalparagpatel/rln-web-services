<h1>Listing partner_org_components</h1>

<table>
  <tr>
    <th>Orig partner org component</th>
    <th>Partner org</th>
    <th>Component type cd</th>
    <th>Display</th>
    <th>Description</th>
    <th>Standard component</th>
    <th>Active ind</th>
    <th>Begin at</th>
    <th>End at</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @partner_org_components.each do |partner_org_component| %>
  <tr>
    <td><%= partner_org_component.orig_partner_org_component_id %></td>
    <td><%= partner_org_component.partner_org_id %></td>
    <td><%= partner_org_component.component_type_cd %></td>
    <td><%= partner_org_component.display %></td>
    <td><%= partner_org_component.description %></td>
    <td><%= partner_org_component.standard_component_id %></td>
    <td><%= partner_org_component.active_ind %></td>
    <td><%= partner_org_component.begin_at %></td>
    <td><%= partner_org_component.end_at %></td>
    <td><%= link_to 'Show', partner_org_component %></td>
    <td><%= link_to 'Edit', edit_partner_org_component_path(partner_org_component) %></td>
    <td><%= link_to 'Destroy', partner_org_component, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

